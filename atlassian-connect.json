{
  "key": "sequence-diagramr",
  "name": "Sequence Diagramr",
  "description": "Generate UML sequence diagrams in Confluence pages",
  "baseUrl": "{{localBaseUrl}}",
  "vendor": {
    "name": "Atlassian Labs",
    "url": "https://www.atlassian.com"
  },
  "links": {
    "documentation": "https://bitbucket.org/atlassianlabs/atlassian-connect-confluence-sequence-diagramr",
    "self": "{{localBaseUrl}}/atlassian-connect.json"
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "staticContentMacros": [
      {
        "name": {
          "value": "Sequence Diagramr"
        },
        "key": "sequence-diagramr",
        "method": "get",
        "url": "/diagram?pageId={page.id}&pageVersion={page.version.id}&userId={user.id}&macroHash={macro.hash}",
        "icon": {
          "url": "/favicon.png"
        },
        "documentation": {
          "url": "https://bitbucket.org/atlassianlabs/atlassian-connect-confluence-sequence-diagramr"
        },
        "outputType": "block",
        "bodyType": "plain-text",
        "featured": true
      }
    ]
  }
}